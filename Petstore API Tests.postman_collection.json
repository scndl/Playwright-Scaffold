                  "});",
                  "",
                  "pm.test(\"Retrieved pet matches created pet ID if exists\", function () {",
                  "    if (pm.response.code === 200) {",
                  "        var jsonData = pm.response.json();",
                  "        var savedPetId = pm.environment.get('petId');",
                  "        if (savedPetId) {",
                  "            pm.expect(jsonData.id).to.eql(parseInt(savedPetId));",
                  "        }",
                  "    } else {",
                  "        console.log('Pet not found - may need to run Create Pet request first');",
                  "    }",
                  "});"
